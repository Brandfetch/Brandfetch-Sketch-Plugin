{
  "name": "brandfetch",
  "version": "1.0.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Brandfetch",
    "manifest": "src/manifest.json",
    "main": "brandfetch.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "regenerator-runtime": "^0.13.3",
    "sketch-module-web-view": "^3.4.1"
  },
  "description": "Speed up your design workflow by accessing brand assets using Brandfetch's plugin right on Sketch",
  "main": "index.js",
  "keywords": [],
  "author": "Brandfetch",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:brandfetch/sketch-plugin-brandfetch.git"
  }
}
