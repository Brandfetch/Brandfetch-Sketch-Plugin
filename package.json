{
  "name": "brandfetch-sketch-plugin",
  "version": "3.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Brandfetch",
    "manifest": "src/manifest.json",
    "main": "brandfetch.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.9.5"
  },
  "dependencies": {
    "sketch-module-web-view": "^3.5.1"
  },
  "description": "Brandfetch is the home for the world's brands. Pull the latest logos, colors, fonts and more directly into Sketch.",
  "main": "index.js",
  "keywords": [],
  "author": "Brandfetch",
  "license": "Copyright Brandfetch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brandfetch/Brandfetch-Sketch-Plugin"
  }
}
